server {
	listen 80;
	server_name mc.apac.external.10xlabs.net mc.apac.internal.10xlabs.net localhost;

	proxy_set_header Host $http_host;
    proxy_redirect off;

	#auth_basic "Restricted";
    #auth_basic_user_file /home/microcloud/htpasswd;

	location / {
    	proxy_pass http://127.0.0.1:8080/;
  }
}